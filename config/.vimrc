set hls
set is
set nowrap
set expandtab
set shiftwidth=2
set softtabstop=2
set smarttab
set autoindent
set smartindent
map <C-T> :vsplit<cr>:CommandT<cr>
map <C-S> :update<cr>
"inoremap { {<CR><BS>}<Esc>ko
"inoremap ( ()<Esc>i
"inoremap " ""<Esc>i
"inoremap ' ''<Esc>i
map <up> <nop>
map <down> <nop>
map <left> <nop>
map <right> <nop>
nnoremap j gj
nnoremap k gk
set pastetoggle=<F2>
